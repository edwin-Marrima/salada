{
  "resources": [
    {
      "type": "aws_s3_bucket",
      "allowed": {
        "when": [
          {
            "resource": "aws_s3_bucket",
            "condition": "exists"
          }
        ]
      },
      "change_actions": {
        "update": {
          "time": "* * * * *"
        }
      },
      "attributes": [
        {
          "name": "bucket_prefix",
          "value": "x"
        }
      ]
    }
  ]
}